{
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.js"
    },
    {
      "src": "/",
      "dest": "apps/web"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ]
}
