{
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "api/index.js"
    },
    {
      "source": "/",
      "destination": "apps/web"
    },
    {
      "source": "/:path*",
      "destination": "apps/web/:path*"
    }
  ]
}
